    "@babel/preset-react": "^7.18.6",
    "@figma/plugin-typings": "^1.51.0",
    "@types/node": "16",
    "babel-loader": "^8.2.5",
    "tailwindcss": "^3.1.8",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
